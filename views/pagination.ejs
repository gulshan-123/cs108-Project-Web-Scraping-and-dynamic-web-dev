<nav aria-label="Page navigation">
    <ul class="pagination pagination-sm justify-content-center">
        <% for(let i = 1; i <= numPages; i++) { %>
            <li class="page-item <%= i === pagenumber ? 'active' : '' %>">
                <a class="page-link" href="/<%= i %>"><%= i %></a>
            </li>
        <% } %>
    </ul>
</nav>